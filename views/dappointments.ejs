<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-flask fa-6x"></i>
    <h1 class="display-3">Doctor Appointments</h1>

    <%appoints.forEach((doctor) => {%>
      <div class="card">
        <p><%=doctor.name%></p>
        <p><%=doctor.time%></p>
        <p><%=doctor.date%></p>
      </div>
    <%});%>
<br>

    <hr>


  </div>
</div>

<%- include('partials/footer') %>
