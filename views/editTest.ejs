<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">Services</h1>

    <%tests.forEach((test) => {%>
      <div class="card">
        <p><%=test.name%></p>
        <p><%=test.cost%></p>
        <a href="/admin/editTest/edit/<%=test._id%>">Edit</a>
        <a href="/admin/editTest/delete/<%=test._id%>">Delete</a>
      </div>

        <%});%>

    <hr>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/admin/addTest" role="button">Add a test</a>
  </div>
</div>

<%- include('partials/footer') %>
