<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-flask fa-6x"></i>
    <h1 class="display-3">Doctor Appointments</h1>

    <%appoints.doctor.forEach((doctor) => {%>
      <div class="card">
        doctor
        <p><%=doctor.name%></p>
        <p><%=doctor.time%></p>
        <p><%=doctor.date%></p>
      </div>
    <%});%>
    <%appoints.test.forEach((test) => {%>
      <div class="card">
        test
        <p><%=test.name%></p>
        <p><%=test.time%></p>
        <p><%=test.date%></p>
      </div>
    <%});%>


  </div>
</div>

<%- include('partials/footer') %>
